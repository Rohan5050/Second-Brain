{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "env": {
    "BACKEND_URL": "https://brainly-backend-livid.vercel.app/api/v1"
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://brainly-backend-livid.vercel.app/api/v1/$1"
    }
  ]
}
